{
  "$schema": "https://market.pluxel.dev/schema/package.json",
  "name": "pluxel-plugin-bot-layer",
  "description": "这是一个示例插件",
  "version": "0.1.0",
  "type": "module",
  "files": [
    "dist",
    "!**/*.map"
  ],
  "main": "./dist/bot-layer.mjs",
  "scripts": {
    "build": "pluxel build"
  },
  "dependencies": {
    "type-flag": "^4.0.2"
  },
  "peerDependencies": {
    "pluxel-plugin-kook": "workspace:^",
    "pluxel-plugin-telegram": "workspace:^"
  },
  "devDependencies": {},
  "module": "./dist/bot-layer.mjs",
  "types": "./dist/bot-layer.d.mts",
  "exports": {
    ".": {
      "@pluxel/hmr": "./src/bot-layer.ts",
      "default": "./dist/bot-layer.mjs"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/bot-layer.mjs",
      "./package.json": "./package.json"
    }
  }
}
