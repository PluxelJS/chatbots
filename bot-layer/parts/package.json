{
  "$schema": "https://market.pluxel.dev/schema/package.json",
  "name": "@pluxel/parts",
  "version": "0.1.0",
  "description": "Parts core model, JSX runtime, and UI helpers.",
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist",
    "!**/*.map"
  ],
  "scripts": {
    "build": "pluxel build"
  },
  "peerDependencies": {
    "@chatui/core": "^3.6.0",
    "@mantine/core": "^8.3.10",
    "@tabler/icons-react": "^3.36.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "peerDependenciesMeta": {
    "@chatui/core": {
      "optional": true
    },
    "@mantine/core": {
      "optional": true
    },
    "@tabler/icons-react": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "exports": {
    ".": {
      "@pluxel/hmr": "./src/index.ts",
      "default": "./dist/index.mjs"
    },
    "./jsx-runtime": {
      "@pluxel/hmr": "./src/jsx-runtime.ts",
      "default": "./dist/jsx-runtime.mjs"
    },
    "./ui-chatui": {
      "@pluxel/hmr": "./src/ui-chatui/index.ts",
      "default": "./dist/ui-chatui/index.mjs"
    },
    "./package.json": "./package.json"
  }
}
