{
  "$schema": "https://market.pluxel.dev/schema/package.json",
  "name": "pluxel-plugin-meme-test",
  "version": "0.1.0",
  "description": "Meme test plugin for chatbots",
  "type": "module",
  "exports": {
    ".": {
      "@pluxel/hmr": "./src/meme-test.ts",
      "default": "./dist/meme-test.mjs"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/meme-test.d.mts",
  "files": [
    "dist",
    "!**/*.map"
  ],
  "scripts": {
    "build": "pluxel build"
  },
  "peerDependencies": {
    "pluxel-plugin-bot-core": "workspace:*",
    "pluxel-plugin-bot-suite": "workspace:*",
    "@pluxel/cmd": "workspace:*",
    "pluxel-plugin-meme-worker": "workspace:*"
  },
  "pluxel": {
    "dependOn": {
      "required": [],
      "optional": [
        "pluxel-plugin-bot-suite",
        "pluxel-plugin-meme-worker"
      ]
    }
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/meme-test.mjs",
      "./package.json": "./package.json"
    }
  }
}
